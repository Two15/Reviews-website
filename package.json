{
  "name": "review-my-code-website",
  "version": "0.0.0",
  "description": "The public face of ReviewMyCode",
  "main": "index.js",
  "scripts": {
    "start": "broccoli serve",
    "build": "rm -fr dist; broccoli build dist",
    "deploy": "npm run build && aws s3 sync dist $S3_BUCKET_URI --acl public-read --cache-control \"public, max-age=86400\" && aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DISTRIBUTION --paths /index.html"
  },
  "author": "Xavier Cambar <xcambar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "broccoli": "^0.16.9",
    "broccoli-env": "0.0.1",
    "broccoli-funnel": "^1.0.3",
    "broccoli-merge-trees": "^1.1.1",
    "broccoli-rename-files": "0.0.3",
    "broccoli-replace": "^0.12.0",
    "broccoli-sass": "^0.7.0",
    "node-uuid": "^1.4.7"
  }
}
